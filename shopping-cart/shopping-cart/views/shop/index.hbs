<div class="row">
  <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3" >
    <div id="success" class="alert alert-succcess" {{#if noMessage}}hidden{{/if}}>
      {{ successMsg }}
    </div>

  </div>
</div>
{{# each products}}
 <div class="row">
  {{# each this}}
   <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{ this.imagePath}}" alt="...">
      <div class="caption">
        <h3>{{ this.title}}</h3>
        <div class='clearfix'>
        <div class='price'>Price ${{ this.price}}</div>
        <div style="text-align:center">
            <a href="/add-to-cart/{{this._id}}" class="btn btn-success" role="button">Add To Chart</a>
        </div>
        </div>
      </div>
    </div>
    </div>
  {{/each}}
</div>

  {{/each}}

  