{{# each products}}
 <div class="row">
  {{# each this}}
   <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{ this.imagePath}}" alt="...">
      <div class="caption">
        <h3>{{ this.title}}</h3>
        <div class='clearfix'>
        <div class='price'>{{ this.price}}</div>
        <div style="text-align:center">
            <a href="#" class="btn btn-success" role="button">View Product</a>
        </div>
        </div>
      </div>
    </div>
    </div>
  {{/each}}
</div>

  {{/each}}

  